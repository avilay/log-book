{
  "cli": {
    "version": ">= 12.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
      	"EXPO_PUBLIC_ENV": "dev",
      	"EXPO_PUBLIC_SIM_DELAY": "false",
        "EXPO_PUBLIC_GEN_DATA": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
      	"EXPO_PUBLIC_ENV": "prod",
      	"EXPO_PUBLIC_SIM_DELAY": "false",
        "EXPO_PUBLIC_GEN_DATA": "false"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6714371529"
      }
    }
  }
}
